    a=    (component "rdfa/ctx-each" model=@model subComponent=(component "rdfa/ctx/a"))
    link= (component "rdfa/ctx-each" model=@model subComponent=(component "rdfa/ctx/link"))
    li=   (component "rdfa/ctx-each" model=@model subComponent=(component "rdfa/ctx/li"))
